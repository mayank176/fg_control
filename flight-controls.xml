      <chunk>
        <name>elevator</name>
        <type>double</type>
        <node>/controls/flight/elevator</node>
      </chunk>
      
      <chunk>
        <name>rudder</name>
        <type>double</type>
        <node>/controls/flight/rudder</node>
      </chunk>
      
      <chunk>
        <name>throttle</name>
        <type>double</type>
        <node>/controls/engines/throttle-all</node>
      </chunk>
    </input>
  </generic>
</PropertyList>
